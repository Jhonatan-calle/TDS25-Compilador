program
{
    integer x = 2;
    integer inc(integer z)
    {
        return z + 1;
    }
    integer get_int() extern;
    void print_int(integer i) extern ;
    void main()
    {
        integer y = 0;
        y = get_int();
        if (y == 1) then    
        {
            print_int(y);
        }
        else
        {
            print_int(inc(y));
            return;
        }
    }
}
